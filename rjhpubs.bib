@Article{SH05,
  Title                    = {Stochastic models underlying Croston's method for intermittent
demand forecasting},
  Author                   = {Shenstone, Lydia and Hyndman, Rob J},
  Year                     = {2005},
  Number                   = {6},
  Pages                    = {389--402},
  Url                      = {dx.doi.org/10.1002/for.963},
  Volume                   = {24},

  Abstract                 = {Croston's method is widely used to predict inventory demand when
it is intermittent. However, it is an ad hoc method with no
properly formulated underlying stochastic model. In this paper,
we explore possible models underlying Croston's method and three
related methods, and we show that any underlying model will be
inconsistent with the properties of intermittent demand data.
However, we find that the point forecasts and prediction
intervals based on such underlying models may still be useful.
},
